{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug main",//here
            "type": "cppdbg",
            "request": "launch",
            // "targetArchitecture": "x64",
            "program": "${workspaceRoot}/main",//here
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "linux": {
                "MIMode": "gdb"
            },
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "gdb",
                "miDebuggerPath": "gdb.exe"
            },
            "setupCommands": [ { "text": "-enable-pretty-printing" } ],
            "preLaunchTask": "Build main"
        }
    ]
}
